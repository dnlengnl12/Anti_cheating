<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dyung.kim.dao.MemberMapper">
<insert id="insertMember" parameterType="member">
	insert into
		Member(
			member_id
			,depart_code
			,school_code
			,member_password
			,member_name
			,member_birth
			,member_phone
			,member_email
			,member_code
			,school_memberCode
		) values (
			#{member_id}
			,#{depart_code}
			,#{school_code}
			,#{member_password}
			,#{member_name}
			,#{member_birth}
			,#{member_phone}
			,#{member_email}
			,#{member_code}
			,#{school_memberCode}
		)
</insert>
</mapper>